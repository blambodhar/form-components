/* Generated by grunt-webfont */
/* Based on https://github.com/endtwist/fontcustom/blob/master/lib/fontcustom/templates/fontcustom.css */

<% if (fontfaceStyles) { %>
<% if (fontPathVariables && stylesheet !== 'css') { %>
<%= fontPathVariable %>
<% } %>
<% if (fontSrc1 && embed.length) { %>
@font-face {
  font-family: "<%= fontFamilyName %>";
  src: <%= fontSrc1 %>;
  font-weight: normal;
  font-style: normal;
}
<% } %>@font-face {
  font-family: "<%= fontFamilyName %>";<% if (fontSrc1) { %>
  src: <%= fontSrc1 %>;<% }%>
  src: <%= fontSrc2 %>;
  font-weight: normal;
  font-style: normal;
}
<% } %>


%icons-before-base {
 &:before {
   font-family: "<%= fontFamilyName %>";
   display: inline-block;
   line-height: 1;
   font-weight: normal;
   font-style: normal;
   speak: none;
   text-decoration: inherit;
   text-transform: none;
   text-rendering: auto;
   -webkit-font-smoothing: antialiased;
   -moz-osx-font-smoothing: grayscale;
 }
}

@mixin <%= fontFamilyName %>-before($icon) {

  @extend %icons-before-base;

<% for (var glyphIdx = 0; glyphIdx < glyphs.length; glyphIdx++) { %><% if (glyphIdx === 0) { %>@if<% } else { %>@else if<% } %> ($icon == "<%= glyphs[glyphIdx] %>") {
  &:before {
    content: "<% if (addLigatures) { %><%= glyphs[glyphIdx] %><% } else { %>\<%= codepoints[glyphIdx] %><% } %>";
  }
}
<% } %>
}

%icons-after-base {
   &:after {
     font-family: "<%= fontFamilyName %>";
     display: inline-block;
     line-height: 1;
     font-weight: normal;
     font-style: normal;
     speak: none;
     text-decoration: inherit;
     text-transform: none;
     text-rendering: auto;
     -webkit-font-smoothing: antialiased;
     -moz-osx-font-smoothing: grayscale;
   }
 }

@mixin <%= fontFamilyName %>-after($icon) {

  @extend %icons-after-base;

<% for (var glyphIdx = 0; glyphIdx < glyphs.length; glyphIdx++) { %><% if (glyphIdx === 0) { %>@if<% } else { %>@else if<% } %> ($icon == "<%= glyphs[glyphIdx] %>") {
  &:after {
    content: "<% if (addLigatures) { %><%= glyphs[glyphIdx] %><% } else { %>\<%= codepoints[glyphIdx] %><% } %>";
  }
}
<% } %>
}
